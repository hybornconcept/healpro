{
	"name": "healnew",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:host": "vite dev --host",
		"build": "vite build",
		"build:analyze": "vite build --mode analyze",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"optimize": "yarn build && yarn preview",
		"perf": "yarn build && lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
		"analyze": "yarn build:analyze && npx vite-bundle-analyzer dist",
		"speed": "yarn build && yarn preview --host",
		"clean": "rimraf node_modules .svelte-kit build dist && yarn install",
		"postinstall": "svelte-kit sync",
		"fresh": "yarn clean && yarn install --force && yarn dev"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-vercel": "^5.5.2",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/postcss": "^4.1.8",
		"@tailwindcss/vite": "^4.0.0",
		"@tanstack/table-core": "^8.21.3",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-shape": "^3.1.0",
		"@types/nprogress": "^0.2.3",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^2.11.0",
		"clsx": "^2.1.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"nprogress": "^0.2.0",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"rimraf": "^6.0.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-toolbelt": "^0.9.1",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"terser": "^5.43.1",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"better-auth": "^1.3.34",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"formsnap": "^2.0.1",
		"layerchart": "^2.0.0-next.34",
		"mode-watcher": "^1.1.0",
		"svelte-sonner": "^1.0.6",
		"sveltekit-superforms": "^2.28.1",
		"zod": "^3.24.1"
	}
}
